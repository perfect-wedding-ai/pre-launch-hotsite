# Google Analytics e Verificação
GOOGLE_ANALYTICS_ID=your-ga-id
GOOGLE_SITE_VERIFICATION=your-verification-code

# URLs da API
NEXT_PUBLIC_API_URL=https://your-api-url.com
NEXT_PUBLIC_BASE_URL=https://your-base-url.com

# Configurações de idioma
NEXT_PUBLIC_DEFAULT_LOCALE=en 

# Outras configurações
NODE_ENV=development

# Netlify (apenas para deploy)
NETLIFY_AUTH_TOKEN=your-netlify-auth-token
NETLIFY_SITE_ID=your-netlify-site-id

# Contentful
CONTENTFUL_SPACE_ID=your-contentful-space-id
CONTENTFUL_ACCESS_TOKEN=your-contentful-access-token
CONTENTFUL_PREVIEW_ACCESS_TOKEN=your-contentful-preview-access-token
CONTENTFUL_REVALIDATE_SECRET=your-contentful-revalidate-secret
CONTENTFUL_ENVIRONMENT_ID=your-contentful-environment-id
CONTENTFUL_MANAGEMENT_TOKEN=your-contentful-management-token
CONTENTFUL_PREVIEW=false